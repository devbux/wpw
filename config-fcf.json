{
  "inbounds": [
    {
      "port": "${PORT}",
      "protocol": "vmess",
      "settings": {
        "clients": [
          {
            "id": "${CLIENT_UUID}"
          }
        ],
        "disableInsecureEncryption": true
      },
      "streamSettings": {
        "network": "ws",
        "security": "none",
        "wsSettings": {
          "path": "${WPATH}",
          "acceptProxyProtocol": false
        }
      }
    }
  ],
  "outbounds": [
    {
      "tag": "direct",
      "protocol": "freedom",
      "settings": {
        "domainStrategy": "UseIP"
      }
    },
    {
      "tag": "blocked",
      "protocol": "blackhole",
      "settings": {}
    },
    {
      "tag": "warp",
      "protocol": "wireguard",
      "settings": {
        "mtu": 1420,
        "secretKey": "${PRIVATE_KEY}",
        "address": [
          "172.16.0.2/32",
          "2606:4700:110:8e84:9bd6:db06:26ec:5fd2/128"
        ],
        "workers": 10,
        "domainStrategy": "ForceIPv4v6",
        "reserved": [
          0,
          0,
          0
        ],
        "peers": [
          {
            "publicKey": "${PUBLIC_KEY}",
            "allowedIPs": [
              "0.0.0.0/0",
              "::/0"
            ],
            "endpoint": "engage.cloudflareclient.com:2408",
            "keepAlive": 0
          }
        ],
        "noKernelTun": false
      }
    }
  ],
  "routing": {
    "domainStrategy": "AsIs",
    "rules": [
      {
        "type": "field",
        "outboundTag": "direct",
        "domain": [
          "engage.cloudflareclient.com"
        ],
        "ip": [
          "1.1.1.1",
          "1.0.0.1",
          "162.159.192.0/18"
        ],
        "port": "53,2408"
      },
      {
        "type": "field",
        "outboundTag": "blocked",
        "ip": [
          "geoip:private"
        ]
      },
      {
        "type": "field",
        "outboundTag": "blocked",
        "protocol": [
          "bittorrent"
        ]
      },
      {
        "type": "field",
        "outboundTag": "blocked",
        "domain": [
          "geosite:category-ads-all",
          "geosite:category-porn",
          "ext:geosite_IR.dat:category-ads-all",
          "ext:geosite_IR.dat:malware",
          "ext:geosite_IR.dat:phishing",
          "ext:geosite_IR.dat:cryptominers"
        ]
      },
      {
        "type": "field",
        "outboundTag": "blocked",
        "ip": [
          "ext:geoip_IR.dat:malware",
          "ext:geoip_IR.dat:phishing"
        ]
      },
      {
        "type": "field",
        "outboundTag": "warp",
        "network": "tcp,udp"
      }
    ]
  },
  "dns": {
    "servers": [
      "94.140.14.14",
      "94.140.15.15"
    ],
    "queryStrategy": "UseIP",
    "tag": "dns_inbound"
  }
}
